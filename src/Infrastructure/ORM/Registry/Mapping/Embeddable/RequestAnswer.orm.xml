<?xml version="1.0" encoding="utf-8" ?>
<doctrine-mapping
        xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                            https://raw.github.com/doctrine/doctrine2/master/doctrine-mapping.xsd">

    <embeddable name="App\Domain\Registry\Model\Embeddable\RequestAnswer">

        <field name="response" column="response" nullable="true"/>
        <field name="date" column="date" type="date" nullable="true"/>
        <field name="type" column="type" nullable="true"/>

    </embeddable>

</doctrine-mapping>
